void showBullets(){
  for(int i = 0 ; i< bullets.size(); i++){
    bullets.get(i).display();
  }
}
void draw(){
  background(#b7b7a4);
  if(gameState.equals("GAME")){
    if(frameCount %60 == 0 && !outOfMaps){
      clock+=1;
    }
    drawNodes(grid);
    showBullets();
    removeDeadBullets();
    //removeBullets();
    textAlign(CENTER);
    textSize(10);
    fill(#FFFFFF);
    String endText = "TOTAL BULLETS USED: " + bulletsUsed;
    text(endText,90,15);
    String timer,score;
    if(clock < 60){
      if(clock>9){
        timer = "0:" + clock;
      }else{timer = "0:0" + clock;}
    }else{
      if(clock%60<10){
        timer = clock/60 +":0" + clock%60;
      }else{timer = clock/60 + ":" + clock%60;}
    }
    score = "YOUR TOTAL SCORE WAS " + playerScore;
    text(timer,90,25);
    if(outOfMaps){
      restartGame.display();
      textAlign(CENTER);
      textSize(10);
      fill(#FFFFFF);
      text(score,90,height - 15);
      if(bulletsUsed < highScore){
        highScore = bulletsUsed;
      }
    }
  }else if(gameState.equals("GAME-OVER")){
    textAlign(CENTER);
    textSize(15);
    fill(#FFFFFF);
    String endText = "ROUND OVER\nIT ONLY TOOK YOU " + bullets.size();
    if(bullets.size() >1){
      endText += " BULLETS";
    }else{
      endText += " BULLET";
    }
    text(endText,width/2,height/2);
    restart.display();
    nextLevel.display();
  }else if(gameState.equals("START")){
    drawNodes(grid);
    showBullets();
    start.display();
    chooseLevel.display();
  }else if(gameState.equals("LEVELS")){
    drawNodes(grid);
    showBullets();
    inc.display();
    dec.display();
    play.display();
    textAlign(CENTER);
    textSize(15);
    fill(#FFFFFF);
    text(level + 1,width/2,height/2);
  }
}

void removeBullets(){
   if(bullets.size() >= 6){
      bullets.remove(0); 
   }
}
boolean intersects(float x,float y,float r,float x2, float y2,float r2){
  return sq(x - x2) + sq(y - y2) < sq(r/2 + r2/2);
}

void removeDeadBullets(){
   for(int i = 0 ; i < bullets.size(); i++){
      if(bullets.get(i).dead){
         bullets.remove(i); 
      }
   }
}

void mousePressed(){
  if(start.mouseOver() && gameState.equals("START")){
    mousePressed = false;
    gameState = "GAME";
    loadNextMap();
  }else if(restart.mouseOver() && gameState.equals("GAME-OVER")){
    mousePressed = false;
    setupNodes(grid);
    startScreen();
    bullets.clear();
    gameState = "START";
  }else if(restartGame.mouseOver() && outOfMaps){
    setup();
  }else if(nextLevel.mouseOver() && gameState.equals("GAME-OVER")){
    mousePressed = false;
    loadNextMap();
    gameState = "GAME";
    mousePressed = false;
  }else if(chooseLevel.mouseOver() && gameState.equals("START")){
    gameState = "LEVELS";
    loadNextMap();
  }else if(inc.mouseOver() && gameState.equals("LEVELS")){
    mousePressed = false;
    level++;
    loadNextMap();
  }else if(dec.mouseOver()&&gameState.equals("LEVELS")){
    mousePressed = false;
    if(level > 0){
      level -= 1;
      loadNextMap();
    }
  }else if(play.mouseOver() && gameState.equals("LEVELS")){
    mousePressed = false;
      gameState = "GAME";
    }
}
