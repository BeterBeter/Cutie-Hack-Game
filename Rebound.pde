void setup(){
  size(1200,810);
  nodeSize = 30;
  grid = new Node[width/nodeSize][height/nodeSize];
  bullets = new ArrayList<Bullet>();
  setupNodes(grid);
  gameState = "START";
  start = new Button("START GAME",width/2-100,height/2+250,125,50,palette[0]);
  restart = new Button("MENU",width/2-100,height/2+50,75,32,palette[0]);
  restartGame = new Button("RESTART",width/2,height/2-50,50,50,palette[0]);
  nextLevel = new Button("NEXT-LEVEL",width/2 + 100,height/2+50,75,32,palette[0]);
