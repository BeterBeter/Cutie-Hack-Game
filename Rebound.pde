  inc = new Button("->",width/2+50,height/2,25,25,palette[0]);
  dec = new Button("<-",width/2-50,height/2,25,25,palette[0]);
  play = new Button("PLAY",width/2,height/2+30,50,25,palette[0]);
  level = 0;
  bulletsUsed = 0;
  outOfMaps = false;
  clock = 0;
  playerScore = 0;
  startScreen();
}
void setupNodes(Node[][] grid){
    for(int i = 0; i< grid.length; i++){
       for(int j = 0 ; j<grid[0].length;j++){
         grid[i][j] = new Node(i*nodeSize,j*nodeSize);
       }
    }
}
void drawNodes(Node[][] grid){
    for(int i = 0; i< grid.length; i++){
       for(int j = 0 ; j<grid[0].length;j++){
         grid[i][j].display();
       }
    }
}
void outOfMaps(){
  endScreen();
  outOfMaps = true;
  playerScore += (1000-bulletsUsed) + (1000-clock);
}
void loadNextMap(){
  setupNodes(grid);
  bullets.clear();
  outOfMaps = false;
  switch(level){
     case 0: loadMap();
     break;
     case 1: loadMap1();
     break;
     case 2: loadMap2();
     break;
     default : outOfMaps();
     break;
  }
}
